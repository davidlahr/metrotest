<div id="<%= dom_id company %>">
  <p>
    <strong>Name:</strong>
    <%= company.name %>
  </p>

  <p>
    <strong>Street address:</strong>
    <%= company.street_address %>
  </p>

  <p>
    <strong>Extra address:</strong>
    <%= company.extra_address %>
  </p>

  <p>
    <strong>City:</strong>
    <%= company.city %>
  </p>

  <p>
    <strong>State:</strong>
    <%= company.state %>
  </p>

  <p>
    <strong>Zip:</strong>
    <%= company.zip %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= company.phone %>
  </p>

  <p>
    <strong>Contact:</strong>
    <%= company.contact %>
  </p>

</div>
